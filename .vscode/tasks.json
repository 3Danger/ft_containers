{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "mak",
			"command": "/usr/bin/make",
			"args": [
				"-C",
				"${workspaceFolder}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/clang++"
		}
	]
}